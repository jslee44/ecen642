                \includegraphics[width=\textwidth]{elimination.jpg}
                \caption{Elimination}
                \label{fig:elimination}
        \end{subfigure}
        
        \caption{\textbf{Automatic elimination og duplicated capillaries}}
\label{fig11}
\end{figure}


